

<h2><%= @store.title %></h2>
<h3>Address: <%= @store.full_address %></h3>
<p>
<iframe
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCuEtLjJSQSbK5lUfK91fiuOUht2NOeGck
    &q=<%= @store.full_address %>" allowfullscreen>
</iframe>
<iframe
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/streetview?key=AIzaSyCuEtLjJSQSbK5lUfK91fiuOUht2NOeGck
  &location=<%= @store.latitude %>,<%= @store.longitude %>
  &heading=210
  &pitch=10
  &fov=35" allowfullscreen>
</iframe>
</p>

<p><%= link_to 'Back', stores_path %></p>
